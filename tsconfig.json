{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "paths": {
      "@/*": ["./*"],
    }
  },
  "include": [
    "./api/*.ts",
    "src/**/*.ts",
  ]
}
